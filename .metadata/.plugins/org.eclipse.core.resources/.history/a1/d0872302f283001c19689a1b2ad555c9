package com.capg.day1;

public class FindEvenAndoddWithRange {
public static void main(String[] args)
{
	for (int i=1;i<=20;i++)
	{
		if (i==10);
		{
			continue;
		}
		if(i%2==0);
		{System.out.println("Even"+i);
			
		}
		else{
			System.out.println("odd"+i);
			
		}
		String[] is = new String[10];
	
	}
	return;
}

}
